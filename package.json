{
  "name": "@davincihealthcare/vue-page-transition",
  "description": "A lean Vue.js plugin for page / route transitions.",
  "version": "1.0.0",
  "author": "Orlando Wenzinger <orlando.wenzinger@bluewin.ch>",
  "keywords": [
    "vue",
    "vuejs",
    "vuejs2",
    "vue plugin",
    "vue route transition",
    "vue router",
    "vue routing",
    "vue transition",
    "vue animation"
  ],
  "homepage": "https://orlandster.github.io/vue-page-transition/#/",
  "bugs": {
    "url": "https://github.com/Orlandster/vue-page-transition/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Orlandster/vue-page-transition.git"
  },
  "license": "MIT",
  "main": "dist/vue-page-transition.common.js",
  "unpkg": "dist/vue-page-transition.browser.js",
  "module": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "node-sass": "^4.0.0",
    "sass-loader": "^4.1.0",
    "vue-loader": "^10.3.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^2.7.0",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "np": "^7.0.0",
    "regenerator-runtime": "^0.13.7"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11",
    "safari >= 5"
  ]
}
